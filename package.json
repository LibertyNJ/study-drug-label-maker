{
  "name": "study-drug-label-maker",
  "productName": "Study Drug Label Maker",
  "version": "1.1.1",
  "description": "Label maker for the research pharmacy program at North Shore University Hospital.",
  "main": "src/main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package --arch=ia32",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --cache --color --ext .jsx,.js src",
    "node-sass": "start node-sass --watch --output-style=compressed ./src/styles/scss/style.scss ./src/styles/style.css"
  },
  "keywords": [],
  "author": "Nathaniel J. Liberty",
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm",
        "appCopyright": ".",
        "asar": true,
        "executableName": "study-drug-label-maker",
        "name": "Study Drug Label Maker",
        "icon": "src/assets/images/icon.ico",
        "win32metadata": {
          "CompanyName": "Northwell Health",
          "FileDescription": "Label maker for the research pharmacy program at North Shore University Hospital."
        }
      },
      "electronWinstallerConfig": {
        "name": "research_label_maker"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "researchlabelmaker"
      }
    }
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "electron-compile": "^6.4.4",
    "electron-devtools-installer": "^2.2.4",
    "electron-squirrel-startup": "^1.0.0",
    "jquery": "^3.4.0",
    "popper.js": "^1.14.7",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.8.4",
    "react-router-dom": "^5.0.0"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "electron-forge": "^5.2.4",
    "electron-prebuilt-compile": "4.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.12.4",
    "node-sass": "^4.12.0"
  }
}
